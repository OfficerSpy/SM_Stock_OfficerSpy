stock TFTeam TF2_GetEnemyTeam(TFTeam team)
{
	if (team == TFTeam_Red)
		return TFTeam_Blue;
	
	if (team == TFTeam_Blue)
		return TFTeam_Red;
	
	return team;
}

stock TFClassType TF2_GetClassIndexFromString(const char[] pClassName, int nLastClassIndex = TF_LAST_NORMAL_CLASS)
{
	for (int i = TF_FIRST_NORMAL_CLASS; i <= nLastClassIndex; i++)
	{
		int classnameLength = strlen(g_aPlayerClassNames_NonLocalized[i]);

		if (strlen(pClassName) < classnameLength)
			continue;

		if (!strcmp(g_aPlayerClassNames_NonLocalized[i], pClassName))
			return view_as<TFClassType>(i);
	}

	return TFClass_Unknown;
}

stock bool TF2_IsHeadshot(int iType)
{
	return iType == TF_CUSTOM_HEADSHOT || iType == TF_CUSTOM_HEADSHOT_DECAPITATION;
}